{
  "name": "WebOS_Angular_app",
  "version": "0.0.1",
  "description": "This is a basic WebOS Angular app",
  "config": {
    "target": "ChibiTV"
  },
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 8080",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli \"**/*.ipk\" \"src/**/*.js\" \"src/**/*.js.map\" \"src/**/*.ngsummary.json\" \"src/**/*.metadata.json\" \"src/**/**/*.ngfactory.ts\" \"src/**/*.ngstyle.ts\" \"src/**/*.shim.ts\"",
    "serve": "lite-server",
    "ares-package": "webostv-app --sdk-cli-path \"D:/webOS_TV_SDK/CLI\" package",
    "ares-install": "webostv-app --sdk-cli-path \"D:/webOS_TV_SDK/CLI\" install",
    "ares-inspect": "webostv-app --sdk-cli-path \"D:/webOS_TV_SDK/CLI\" inspect",
    "build:prod:install": "npm run build:prod && npm run meta && npm run package && npm run inst && npm run inspect",
    "meta": "webostv-app meta dist",
    "package": "npm run ares-package \"dist/\"",
    "inst": "npm run ares-install -- -d %npm_package_config_target%",
    "inspect": "npm run ares-inspect -- -o -d %npm_package_config_target%"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChibiTomo/WebOS_Angular_app.git"
  },
  "author": "ChibiTomo <thomias.yannis@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ChibiTomo/WebOS_Angular_app",
  "dependencies": {
    "@angular/animations": "~7.0",
    "@angular/common": "~7.0",
    "@angular/compiler": "~7.0",
    "@angular/compiler-cli": "~7.0",
    "@angular/core": "~7.0",
    "@angular/forms": "~7.0",
    "@angular/http": "~7.0",
    "@angular/platform-browser": "~7.0",
    "@angular/platform-browser-dynamic": "~7.0",
    "@angular/platform-server": "~7.0",
    "@angular/router": "~7.0",
    "@angular/upgrade": "~7.0",
    "core-js": "~2.5",
    "file-system": "^2.2.2",
    "rxjs": "~6.3",
    "zone.js": "~0.8"
  },
  "devDependencies": {
    "@ngtools/webpack": "~7.0",
    "@types/core-js": "~2.5",
    "@types/node": "~10.12",
    "angular-router-loader": "~0.8",
    "angular2-template-loader": "~0.6",
    "awesome-typescript-loader": "~5.2",
    "babel-core": "~6.26",
    "babel-loader": "~7.1",
    "babel-preset-env": "~1.7",
    "babel-preset-es2015": "~6.24",
    "clean-webpack-plugin": "~0.1",
    "commander": "^2.19.0",
    "cross-env": "~5.2",
    "css-loader": "~1.0",
    "cssnano": "~4.1",
    "del-cli": "~1.1",
    "html-loader": "~0.5",
    "html-webpack-plugin": "~3.2",
    "lite-server": "~2.4",
    "node-sass": "~4.9",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "sass-loader": "~7.1",
    "to-string-loader": "~1.1",
    "typescript": "~3.1",
    "uglifyjs-webpack-plugin": "~2.0",
    "webos-meta-webpack-plugin": "^0.3.1",
    "webostv-app": "0.0.4",
    "webpack": "~4.29",
    "webpack-cli": "~3.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "~4.2"
  }
}
